generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Schedule {
  key   String @unique
  value String
}

model telegrafSessions {
  key     String @unique
  session String

  @@map("telegraf-sessions")
}

model User {
  id         Int    @id
  first_name String @default("")
  last_name  String @default("")
  username   String @default("")

  role Roles @default(USER)
}

enum Roles {
  USER
  ADMIN
  SUPER_ADMIN
}
